<h1>Create a New Cocktail</h1>
<%= simple_form_for(@cocktail) do |f| %>
  <%= f.error_notification %>

  <%= f.input :name, autofocus: true %>
  <%= f.input :about %>
  <%= f.input :instructions %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>

  <%= f.button :submit %>
<% end %>
